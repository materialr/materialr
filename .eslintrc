{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "function-paren-newline": ["error", "consistent"],
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/label-has-for": ["error", { "required": { "every": ["id"] } }],
    "object-curly-newline": ["error", { "consistent": true }]
  },
  "settings": {
    "import/resolver": {
      "webpack": { "config": ".webpack/webpack.config.js" }
    }
  },
}
